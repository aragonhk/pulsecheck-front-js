{
  "name": "pulsecheck-front-js",
  "version": "1.0.0",
  "description": "Pulsecheck front-end",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist --domain lively-horse.surge.sh"
  },
  "author": "Michael Ng",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.5",
    "eslint": "^4.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.2.1",
    "eslint-watch": "^3.1.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jsdom": "^11.1.0",
    "json-schema-faker": "^0.4.6",
    "mocha": "^3.5.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.7.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.6.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.0",
    "cldr-data": "^31.0.2",
    "expect": "^1.20.2",
    "file-saver": "^1.3.3",
    "globalize": "^1.3.0",
    "helmet": "^3.8.1",
    "jquery": "^3.2.1",
    "json2csv": "^3.11.5",
    "jwt-decode": "^2.2.0",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-chartjs-2": "^2.6.4",
    "react-cookie": "^2.1.1",
    "react-dom": "^15.6.2",
    "react-file-input": "^0.2.5",
    "react-file-reader": "^1.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "react-simpletabs": "^0.7.0",
    "react-widgets": "^4.1.0",
    "react-widgets-globalize": "^4.0.4",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8",
    "toastr": "^2.1.2"
  }
}
